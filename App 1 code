onEvent("FRESHFADE", "click", function( ) {
	console.log("FRESHFADE clicked!");
	setScreen("Freshcut");
	playSound("Pop-Bubble-SFX.mp3");
});
onEvent("SALTY", "click", function( ) {
	console.log("SALTY clicked!");
	setScreen("Salty");
	playSound("Pop-Bubble-SFX.mp3");
});
