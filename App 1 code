//basic page functions
onEvent("Arapaima", "mouseover", function( ) {
  setSize("Arapaima", 275, 120);
  
	console.log("Arapaima hovered!");
});
onEvent("Arapaima", "mouseout", function( ) {
  setSize("Arapaima", 250, 100);
  
	console.log("Arapaima hovered!");
});
onEvent("Arapaima", "click", function( ){
  setScreen("Arapaima_Screen");
  console.log("Arapaima clicked!"); 
  playSound("Pop-Bubble-SFX.mp3");
});
//end of arapaima
onEvent("Largemouth_Bass", "mouseover", function( ) {
  setSize("Largemouth_Bass", 175, 100);
  
	console.log("Largemouth_Bass hovered!");
});
onEvent("Largemouth_Bass", "mouseout", function( ) {
  setSize("Largemouth_Bass", 150, 75);
  
	console.log("Largemouth_Bass hovered!");
});
onEvent("Largemouth_Bass", "click", function( ){
  setScreen("Largemouth_Bass_Screen");
  playSound("Pop-Bubble-SFX.mp3");
  console.log("Largemouth_Bass clicked!"); 
});
//end of bass

//end of freshwater fish functions
onEvent("Clownfish", "mouseover", function( ) {
  setSize("Clownfish", 225, 125);
  
	console.log("Clownfish hovered!");
});
onEvent("Clownfish", "mouseout", function( ) {
  setSize("Clownfish", 200, 100);
  
	console.log("Clownfish hovered!");
});
onEvent("Clownfish", "click", function( ){
  playSound("Pop-Bubble-SFX.mp3");
  setScreen("Clownfish_Screen");
  console.log("Clownfish clicked!"); 
});
